{
    "Properties": {
        

        "chart": {
            "zoomType": "x"
        },
        
        "title": {
            "text": "Wind Direction"
        },
        "subtitle": {
            "text": "Source: Steve's Email"
        },

        "plotOptions": {
            "line": {
                "marker": {
                    "enabled": false,
                    "radius": 2
                },
                "states": {
                    "hover": {
                        "lineWidth": 2
                    }
                }
                
            }
        },



        "xAxis": {
            "gridLineWidth": 1,
            "type": "datetime",
            "labels": {
                "format": "{value:%Y/%b/%e %I:%M:%S}",
                "padding": 10
            },
            "title": {
                "text": "date"
            }
            
        },

        "yAxis": [
            {
            "labels": {
                "offset": 10,
                "format": "{value}Â°"
            },
            "title": {
                "text": "Degree"
            },
            "opposite": false,
            "max": 360,
            "tickInterval": 45,
            "endOnTick": false

        }, {
            "labels": {
                "offset": 10,
                "format": "{value}"
            },
            "type": "category",
            "title": {
                "margin": 60,
                "text": "Cardinal Direction"
            },
            "opposite": true
            

        }],

        "series": [
            {
   
            "type": "line",
            "name": "Wind Direction"

            }
        ],
        "tooltip": {
            "pointFormat": "<span style=\"color:{series.color}\">{series.name}</span>: <b>{point.y}</b>",

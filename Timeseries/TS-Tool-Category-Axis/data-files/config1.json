{
    "Properties": {
        

        "chart": {
            "zoomType": "x"
        },
        
        "title": {
            "text": "Wind Direction"
        },
        "subtitle": {
            "text": "Source: Steve's Email"
        },

        "plotOptions": {
            "line": {
                "marker": {
                    "enabled": false,
                    "radius": 2
                },
                "states": {
                    "hover": {
                        "lineWidth": 2
                    }
                }
                
            }
        },

        "xAxis": {
            "gridLineWidth": 1,
            "type": "datetime",
            "labels": {
                "format": "{value:%Y/%b/%e %I:%M:%S}",
                "padding": 10
            },
            "title": {
                "text": "date"
            }
  
        },

        "yAxis": [
            {
            "labels": {
                "offset": 10,
                "format": "{value}Â°"
            },
            "title": {
                "text": "Degree"
            },
            "opposite": false,
            "max": 360,
            "tickInterval": 45,
            "endOnTick": false,

            "plotBands": [{
                "from": 0,
                "to": 0,
                "color": "black",
                "label": {
                    "text": "North",
                    "style": {
                        "color": "#606060"
                    },
                    "align": "right",
                    "x": 40
                }
                }, {
                "from": 90,
                "to": 90,
                "color": "black",
                "label": {
                    "text": "East",
                    "style": {
                        "color": "#606060"
                    },
                    "align": "right",
                    "x": 40
                }
                }, {
                "from": 180,
                "to": 180,
                "color": "black",
                "label": {
                    "text": "South",
                    "style": {
                        "color": "#606060"
                    },
                    "align": "right",
                    "x": 40
                }
                }, {
                "from": 270,
                "to": 270,
                "color": "black",
                "label": {
                    "text": "West",
                    "style": {
                        "color": "#606060"
                    },
                    "align": "right",
                    "x": 40
                }
                }, {
                "from": 360,
                "to": 360,
                "color": "black",
                "label": {
                    "text": "North",
                    "style": {
                        "color": "#606060"
                    },
                    "align": "right",
                    "x": 40
                }
            }]

        }, {
            "labels": {
                "offset": 10,
                "format": "{value}"
            },
            "title": {
                "margin": 60,
                "text": "Cardinal Direction"
            },
            "opposite": true
        }],

        "series": [
            {
   
            "type": "line",
            "name": "Wind Direction"

            }
        ],
        "tooltip": {
            "pointFormat": "<span style=\"color:{series.color}\">{series.name}</span>: <b>{point.y}</b>",
            "valueDecimals": 2,
            "split": true
        },

        "legend": {
            "enabled": true
        },
        
        "navigator": {
            "enabled": false
        }
    }
}